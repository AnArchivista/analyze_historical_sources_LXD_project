<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <preview>
    <thumbnail>thumbnail.jpg</thumbnail>
    <title>Full Screen</title>
    <description><![CDATA[This interface provides a simple layout that has a header, with navigation, included and adjusts to the height and width of the device.]]></description>
    <specialNotes />
  </preview>
  <html>
    <bgColor>#ffffff</bgColor>
  </html>
  <config>
    <isEditable>false</isEditable>
  </config>
  <layout>
    <headerTopMargin />
    <headerRightMargin />
    <headerBottomMargin />
    <headerLeftMargin />
    <viewerTopMargin />
    <viewerRightMargin />
    <viewerBottomMargin />
    <viewerLeftMargin />
    <navigatorTopMargin />
    <navigatorRightMargin />
    <navigatorBottomMargin />
    <navigatorLeftMargin />
    <headerTopPadding />
    <headerRightPadding />
    <headerBottomPadding />
    <headerLeftPadding />
    <viewerTopPadding />
    <viewerRightPadding />
    <viewerBottomPadding />
    <viewerLeftPadding />
    <navigatorTopPadding />
    <navigatorRightPadding />
    <navigatorBottomPadding />
    <navigatorLeftPadding />
    <bgImage>Burlap_Background_Image.jpg</bgImage>
  </layout>
  <htmlTemplateStyle>
    <background url="background.jpg" alpha="1" color="" repeat="yes" position="top" />
    <titleText color="#ffffff" size="2em" fontFamily="Arial, Helvetica, sans-serif" />
    <bodyText_1 color="#ffffff" size="1em" />
    <bodyText_2 color="#7f807a" size="1em" />
    <links normal="#0000FF" normalDecoration="underline" visited="#808080" visitedDecoration="underline" hover="#808080" hoverDecoration="underline" />
    <customNav color="#7f807a" />
  </htmlTemplateStyle>
  <object>
    <height>470</height>
    <width>800</width>
    <parameters>
      <parameter><![CDATA[<PARAM NAME="Src" VALUE="os.swf">]]></parameter>
    </parameters>
  </object>
  <interaction>
    <!--  This is the title put in the header. Not all interfaces will have a geader, in fact, the default one will not. 	-->
    <title />
    <titleColor>#FFFFFF</titleColor>
    <headerFileName>header.jpg</headerFileName>
    <footerFileName>footer.jpg</footerFileName>
  </interaction>
</interface>
